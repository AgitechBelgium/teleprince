<odoo>
    <data>
        <record id="res_config_settings_view_form_ext" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit.base.setup.ext</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="base_setup.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@id='company_details_settings']" position="inside">
                    <div class="o_setting_right_pane row">
                        <span class="o_form_label">Teleprince Logo</span>
                        <field name="teleprince_logo" widget="image" class="oe_avatar"/>
                    </div>
                    <br/>
                    <div class="o_setting_right_pane row">
                        <span class="o_form_label">Teleprince &amp; B&amp;O Logo</span>
                        <field name="b_and_o_logo" widget="image" class="oe_avatar"/>
                    </div>
                    <br/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>